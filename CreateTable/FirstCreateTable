create table internTable
(
ID number,
FIRST_NAME VARCHAR2(50),
LAST_NAME VARCHAR2(50),
UNIVERSITY VARCHAR2(50),
DEPARTMENT VARCHAR2(50),
CLASS NUMBER,
CITY VARCHAR2(50)
);

SELECT * FROM internTable;


